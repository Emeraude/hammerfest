<swf>

	<mtypes file="swf/_code.swf">
		<classpath value="flash7"/>
		<classpath value="lib"/>
		<classpath value="class/hammer"/>
		<file value="GameManager.mt"/>
	</mtypes>

	<base file="swf/_code.swf" version="8"/>

	<include file="swf/_gfx.swf" link="*"/>
	<include file="swf/_editor.swf" link="*"/>
	<include file="swf/_anims.swf" link="*"/>
	<include file="swf/_sounds.swf" link="*"/>
	<include file="swf/_ending.swf" link="*"/>

	<!-- Base Levels -->
	<xml file="xml/levels/adventure.xml" var="xml_adventure"/>
	<xml file="xml/levels/tutorial.xml" var="xml_tutorial"/>
	<xml file="xml/levels/shareware.xml" var="xml_shareware"/>
	<xml file="xml/levels/fjv.xml" var="xml_fjv"/>
	<xml file="xml/levels/timeAttack.xml" var="xml_time"/>
	<xml file="xml/levels/multiTime.xml" var="xml_multitime"/>
	<xml file="xml/levels/soccer.xml" var="xml_soccer"/>
	<xml file="xml/levels/hof.xml" var="xml_hof"/>

	<!-- Extra levels -->
	<xml file="xml/levels/deepnight.xml" var="xml_deepnight"/>
	<xml file="xml/levels/hiko.xml" var="xml_hiko"/>
	<xml file="xml/levels/ayame.xml" var="xml_ayame"/>
	<xml file="xml/levels/hk.xml" var="xml_hk"/>

	<!-- Dev levels -->
	<xml file="xml/levels/dev.xml" var="xml_dev"/>
	<xml file="xml/levels/test.xml" var="xml_test"/>

	<!-- Items -->
	<xml file="xml/specialItems.xml" var="xml_specialItems"/>
	<xml file="xml/scoreItems.xml" var="xml_scoreItems"/>

	<!-- Misc -->
	<xml file="xml/portalLinks.xml" var="xml_portalLinks"/>
	<xml file="xml/quests.xml" var="xml_quests"/>

	<output file="swf/game.swf" zip="true"/>

</swf>
